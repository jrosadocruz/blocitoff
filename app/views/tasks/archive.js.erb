<% if @task.archived? %>
  console.log('jose');
  $('#task-' + <%= @task.id %>).slideToggle(function(){
    $(this).remove();
  });
  $('#task-count').html("<%= @current_user.tasks.where(archived: false).count %>");
  $('#errors').html("<%= j(render partial: 'shared/notifications') %>");
<% else %>
  $('#errors').html("<%= j(render partial: 'shared/notifications') %>");
<% end %>