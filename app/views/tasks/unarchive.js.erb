<% if !@task.archived? %>
  console.log('unarchive-page');
  $('#task-' + <%= @task.id %>).slideToggle(function(){
    $(this).remove();
  });
  $('#task-count').html("(<%= @current_user.tasks.where(archived: true).count %>)");
  $('#errors').html("<%= j(render partial: 'shared/notifications') %>");
<% else %>
  $('#errors').html("<%= j(render partial: 'shared/notifications') %>");
<% end %>